{
	"name": "{{ project_name }}",
	"description": "{{ description }}",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint ."
	},
	"devDependencies": {
		"@eslint/compat": "^1.2.5",
		"@eslint/js": "^9.18.0",
		"@sveltejs/adapter-auto": "^6.0.0",
		"@sveltejs/kit": "^2.22.0",
		"@sveltejs/vite-plugin-svelte": "^6.0.0",
		"@tailwindcss/vite": "^4.0.0",
		"eslint": "^9.18.0",
		"eslint-config-prettier": "^10.0.1",
		"eslint-plugin-svelte": "^3.0.0",
		"globals": "^16.0.0",
		"prettier": "^3.4.2",
		"prettier-plugin-svelte": "^3.3.3",
		"prettier-plugin-tailwindcss": "^0.6.11",
		"svelte": "^5.0.0",
		"svelte-check": "^4.0.0",
		"tailwindcss": "^4.0.0",
		"typescript": "^5.0.0",
		"typescript-eslint": "^8.20.0",
		"vite": "^7.0.4"{% if include_pwa %},
		"@serwist/vite": "^10.0.0",
		"@serwist/workbox": "^10.0.0",
		"serwist": "^10.0.0"{% endif %}{% if ui_library == "svelteui" %},
		"@svelteuidev/core": "^0.15.9"{% endif %}{% if ui_library == "skeleton" %},
		"@skeletonlabs/skeleton": "^2.8.0"{% endif %}{% if ui_library == "carbon" %},
		"carbon-components-svelte": "^0.85.0"{% endif %}{% if "database" in integrations %},
		"@prisma/client": "^5.9.1",
		"prisma": "^5.9.1"{% endif %}{% if "auth" in integrations %},
		"@auth/sveltekit": "^0.18.6"{% endif %}{% if "analytics" in integrations %},
		"@vercel/analytics": "^1.1.2"{% endif %}{% if "cms" in integrations %},
		"@sanity/client": "^6.12.3"{% endif %}
	}
}
